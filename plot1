dados <- read.table("c:/rdatas/household_power_consumption.txt",head=TRUE,sep=";")
library(lubridate)
datas <- dmy(dados$Date)
lista <- datas>=dmy("01/02/2007")&datas<=dmy("02/02/2007")
novosdados <- subset(dados, lista)
newgap <- as.numeric(as.character(novosdados$Global_active_power))
png(filename="c:/rdatas/file01.png")
hist(newgap, col="red", xlab="Global Active Power (kilowatts)", main="Global Active Power")
dev.off()
